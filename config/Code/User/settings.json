{
  "vim.hlsearch": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.vscode": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true
  },
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 100,
  "vim.textwidth": 100,
  "workbench.editor.enablePreview": false,
  "auto-close-tag.activationOnLanguage": ["html", "xml"],
  "window.zoomLevel": 0,
  "vim.enableNeovim": true,
  "vim.neovimPath": "/usr/bin/nvim",
  "editor.minimap.enabled": false,
  "problems.autoReveal": false,
  "editor.renderWhitespace": "all",
  "search.useIgnoreFiles": true,
  "material-icon-theme.showUpdateMessage": false,
  "editor.fontFamily": "'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "workbench.iconTheme": "material-icon-theme",
  "editor.formatOnSave": true,
  "workbench.colorTheme": "Palenight Theme",
  "workbench.colorCustomizations": {
    "editor.background": "#19162c",
    "sideBar.background": "#110f1a",
    "sideBar.border": "#00000000",
    "activityBar.background": "#0b0a0f",
    "activityBar.border": "#00000000",
    "input.background": "#1d1b28"
  },
  "typescript.disableAutomaticTypeAcquisition": true,
  "telemetry.enableTelemetry": false,
  "typescript.tsserver.log": "verbose",
  "typescript.tsdk": "./node_modules/typescript/lib",
  "breadcrumbs.enabled": true,
  "javascript.suggest.autoImports": false,
  "editor.snippetSuggestions": "none",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.startupEditor": "newUntitledFile",
  "window.titleBarStyle": "custom",
  "emmet.showAbbreviationSuggestions": false,
  "emmet.showExpandedAbbreviation": "never",
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "references.preferredLocation": "view",
  "gitlens.defaultDateFormat": "Y-MM-DD HH:mm",
  "dart.flutterCreateAndroidLanguage": "kotlin",
  "dart.flutterCreateIOSLanguage": "swift"
}
